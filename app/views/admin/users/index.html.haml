%h1 Listing users
%table.table-striped
  %tr
    %th Email
    %th Phone
    %th Code
    %th Address
    %th
    %th
    %th
  - @users.each do |user|
    %tr
      %td= user.email
      %td= user.phone
      %td= user.code
      %td= user.address
      %td= link_to 'Show', admin_user_path(user)
      %td= link_to 'Edit', edit_admin_user_path(user)
      %td= link_to 'Destroy', [:admin, user], method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New User', new_admin_user_path
